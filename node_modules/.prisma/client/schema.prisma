generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Contact {
  id        Int      @id @default(autoincrement())
  first     String?
  last      String?
  avatar    String?
  twitter   String?
  notes     String?
  favorite  Boolean?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
